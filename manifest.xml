<manifest version="2.0" type="device" target-level="5">
    <!-- Declares the Framework Compatibility Matrix (FCM) level this device is compatible with -->
    <fcm_version>5</fcm_version> <!-- Android 10 shipped devices = FCM 5 -->

    <!-- Kernel FCM level (optional since kernel version is known) -->
    <!-- <kernel target-level="5" /> -->

    <sepolicy>
        <version>202404</version>
    </sepolicy>

    <!-- Vendor NDK and System SDK version -->
    <vendor-ndk>
        <version>29</version>
    </vendor-ndk>
    <system-sdk>
        <version>29</version>
    </system-sdk>
</manifest>
